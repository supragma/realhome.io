 <br>
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-6">
        <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
          <div class="carousel-inner">
             <% @property.property_images.each_with_index do |p_image, index| %>
              <div class="carousel-item <%= index == 0 ? 'active' : '' %>">
                <%= image_tag p_image,class: "d-block w-100" %>
              </div>
            <% end %>
          </div>
        </div>
      </div>
       <div class="col-md-4">
          <h2 style="text-align:center;">Ammenities</h2>
          <div class="price">$<%= @property.monthly_rent %></div>
          <span><i class="fa fa-arrows-alt"></i><%= @property.area_in_square_feet %> Sq ft</span>
          <span><i class="fa fa-bath"></i><%= @property.bedrooms %> Bedrooms</span>
          <span><i class="fa fa-arrows-alt"></i>Longitude: <%= @property.longitude %></span>
          <span><i class="fa fa-bath"></i>Latitude:<%= @property.latitude %></span>
          <div class="address"><%= @property.address %></div>
        </div>
      <div class="row">
        <div class="col-md-12">
           <%= image_tag "http://maps.googleapis.com/maps/api/staticmap?center=#{@property.latitude},#{@property.longitude}&markers=#{@property.latitude},#{@property.longitude}&zoom=7&size=1000x400&key=AIzaSyBao0kOCqbC72yfqXUwN14QhBrSZbXHRWo",
          class: 'img-fluid img-rounded', alt: "#{@property.address} on the map"%>
        </div>
      </div>
    </div>
  </div>








